<h1>Create an Account</h1>

<form>
  <div>
    <label for="username">Username</label>
    <input type="text"/>
  </div>
  <div>
    <label for="email">Email</label>
    <input type="text"/>
  </div>
  <div>
    <label for="password">Password</label>
    <input type="password"/>
  </div>
  <div>
    <button type="submit">Create Account</button>
  </div>
</form>

<script src="/public/js/signup.js"></script>